# OpenRouter API (for Gemini access - FREE!)
# Get from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google OAuth (Required for login)
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# LinkedIn OAuth (Optional - for direct posting)
# Get from: https://www.linkedin.com/developers/apps
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret

# Supabase Database (for email collection)
# Get from: https://supabase.com/dashboard/project/_/settings/api
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here

# Server Config
JWT_SECRET=change_this_to_a_random_string_in_production
FRONTEND_URL=https://gptwrappedd.netlify.app
BACKEND_URL=https://your-backend.up.railway.app
PORT=3001

